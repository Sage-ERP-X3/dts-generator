{
	"version": "3.1.6",
	"compilerOptions": {
		"target": "es2015",
		"module": "umd",
		"declaration": false,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedParameters": true,
		"strictNullChecks": false,
		"moduleResolution": "node"
	},
	"include": [
		"./*.ts",
		"./bin/**/*.ts",
		"./node_modules/typescript/lib/typescript.d.ts",
		"./node_modules/typescript/lib/typescript.ts"
	]
}
